    <g data-itemid="sbv-video-unmute-icon">
        <path d="m 38,26 a 20 12 0 0 1 0 12 l 2 2 a 40 20 0 0 0 0 -16 z"></path>
        <path d="m 43,21 a 48 25 0 0 1 0 22 l 2 2 a 53 29 0 0 0 0 -26 z"></path>
    </g>
</svg>

            </div>
        </button>
    
</div>

                    </div></div>
